{
  "name": "twallet-mini-app-ts3-statement",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.1",
    "@testing-library/react": "10.4.7",
    "@testing-library/user-event": "12.0.11",
    "@types/jest": "24.9.1",
    "@types/node": "12.12.51",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-typescript-sort-keys": "1.3.0",
    "import-sort-style-eslint": "6.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "react-scripts": "3.4.1",
    "typescript": "3.9.7"
  },
  "scripts": {
    "build": "react-scripts build",
    "eslint-print-config": "eslint --print-config src/index.tsx > misc/eslint-print-config.json",
    "lint-dry": "npm-run-all --parallel --continue-on-error lint:eslint-dry lint:tsc",
    "lint:eslint-dry": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint:eslint": "eslint -c .eslintrc.js --fix --ext .ts,.tsx,.js src",
    "lint:tsc": "tsc --noEmit",
    "lint": "npm-run-all --parallel --continue-on-error lint:eslint lint:tsc",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  }
}
